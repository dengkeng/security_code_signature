<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>/clouddragonData/.m2/repository</localRepository>
    <mirrors>
        <mirror>
            <id>artifactory-maven-group</id>
            <mirrorOf>*,!artifactory-maven,!artifactory-plugin,!huawei-central-maven,!huawei-central-plugin,!artifactory-thirdparty-maven</mirrorOf>
            <name>Artifactory Maven</name>
            <url>https://cmc.centralrepo.rnd.huawei.com/maven/</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>internal</id>
            <repositories>
                <repository>
                    <id>huawei-central-maven</id>
                    <url>https://cmc.centralrepo.rnd.huawei.com/maven/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>huawei-central-plugin</id>
                    <name>huawei central plugin</name>
                    <url>https://cmc.centralrepo.rnd.huawei.com/maven/</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>internal</activeProfile>
    </activeProfiles>
</settings>